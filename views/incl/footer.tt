    </div>

    <div class="span3">
      <div id="right-side">
        <% IF conf.show_right %>
         <% IF status != 'show' %>
          <span class="label label-warning"><% status %></span>
         <% END %>

            <div id="newsletter-right-form">
               <h3><a href="/perl-training-consulting">Training and Consulting</a></h3>
            </div>

           <% INCLUDE incl/social_share.tt %>
           <% INCLUDE incl/adsense_tall.tt %>

           <% IF conf.show_related && related %>
             <div id="related">
               <b>Related articles</b>
               <ul>
                 <% FOR l IN related %>
                   <li><a href="<% l.url %>"><% l.text %></a></li>
                 <% END %>
               </ul>
             </div>
           <% END %>

         <% END %>
      </div>
    </div><!-- end of right column -->
  </div>
